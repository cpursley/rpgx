worker_processes  8;
error_log logs/error.log;

events {
  worker_connections 1024;
}

http {
  upstream database {
    postgres_server 127.0.0.1 dbname=<%= Config[:database][:name] %> user=<%= Config[:database][:user] %> password=<%= Config[:database][:password]%>;
  }

  server {
    listen 8888;

  <% @partials.each do |partial| %><%= partial %><% end %>
  <%= @error_codes %>
  }
}
